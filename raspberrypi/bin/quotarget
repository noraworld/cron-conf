#!/bin/sh

# The positive value indicates that --checked is specified while the negative one means that --unchecked is assigned.
# For instance, the script returns true (1)
# if +6 & -4 ("--checked 6 --unchecked 4") is specified
# and the number of checked marks is more than or equal to 6 and the number of unchecked marks is less than or equal to 4.
#
# | Service                  | Timeframe | Daily+  | Daily-  | Perso | Work  | Past  | Same as (on purpose)  |
# | ------------------------ | :-------: | :-----: | :-----: | :---: | :---: | :---: | --------------------- |
# | Discipline               | day       |      +4 |   n/a   |  n/a  |  n/a  |     0 |                       |
# | Discipline               | night     |      +6 |   n/a   |  n/a  |  n/a  |     0 |                       |
# | Discipline               | midnight  |     +10 |   n/a   |  n/a  |  n/a  |     0 |                       |
# | ------------------------ | --------- | ------- | ------- | ----- | ----- | ----- | --------------------- |
# | Gaming                   | day       |     +12 |      -0 |  ALL  |  ALL  |     1 | Deregulated Superman  |
# | Gaming                   | night     |     +12 |      -0 |  ALL  |  ALL  |     1 | Deregulated Superman  |
# | Gaming                   | midnight  |     +12 |      -0 |  ALL  |  ALL  |     1 | Deregulated Superman  |
# | Netflix                  | day       |     +12 |      -0 |  ALL  |  ALL  |     2 | VOD, Ometeotl         |
# | Netflix                  | night     |     +12 |      -0 |  ALL  |  ALL  |     2 | VOD, Ometeotl         |
# | Netflix                  | midnight  |     +12 |      -0 |  ALL  |  ALL  |     2 | VOD, Ometeotl         |
# | Social Media             | day       |     +12 |      -0 |  ALL  |  ALL  |     3 | TBA                   |
# | Social Media             | night     |     +12 |      -0 |  ALL  |  ALL  |     3 | TBA                   |
# | Social Media             | midnight  |     +12 |      -0 |  ALL  |  ALL  |     3 | TBA                   |
# | VOD                      | day       |     +12 |      -0 |  ALL  |  ALL  |     2 | Netflix, Ometeotl     |
# | VOD                      | night     |     +12 |      -0 |  ALL  |  ALL  |     2 | Netflix, Ometeotl     |
# | VOD                      | midnight  |     +12 |      -0 |  ALL  |  ALL  |     2 | Netflix, Ometeotl     |
# | YouTube                  | day       |     +12 |      -0 |  ALL  |  ALL  |     3 | Discipline, Superman  |
# | YouTube                  | night     |     +12 |      -0 |  ALL  |  ALL  |     3 | Discipline, Superman  |
# | YouTube                  | midnight  |     +12 |      -0 |  ALL  |  ALL  |     3 | Discipline, Superman  |
# | YouTube Alternative      | day       |     +12 |      -0 |  ALL  |  ALL  |     3 | Maeve, Upstream       |
# | YouTube Alternative      | night     |     +12 |      -0 |  ALL  |  ALL  |     3 | Maeve, Upstream       |
# | YouTube Alternative      | midnight  |     +12 |      -0 |  ALL  |  ALL  |     3 | Maeve, Upstream       |
# | YouTube Music Persistent | day       |      +4 |   n/a   |  n/a  |  n/a  |     0 |                       |
# | YouTube Music Persistent | night     |      +6 |   n/a   |  n/a  |  n/a  |     0 |                       |
# | YouTube Music Persistent | midnight  |     +10 |   n/a   |  n/a  |  n/a  |     0 |                       |
# | ------------------------ | --------- | ------- | ------- | ----- | ----- | ----- | --------------------- |
# | Deregulated Superman     | day       |     +12 |      -0 |  ALL  |  ALL  |     2 | Gaming                |
# | Deregulated Superman     | night     |     +12 |      -0 |  ALL  |  ALL  |     2 | Gaming                |
# | Deregulated Superman     | midnight  |     +12 |      -0 |  ALL  |  ALL  |     2 | Gaming                |
# | Maeve                    | day       |     +12 |      -0 |  ALL  |  ALL  |     3 | Alternative, Upstream |
# | Maeve                    | night     |     +12 |      -0 |  ALL  |  ALL  |     3 | Alternative, Upstream |
# | Maeve                    | midnight  |     +12 |      -0 |  ALL  |  ALL  |     3 | Alternative, Upstream |
# | Ometeotl                 | day       |     +12 |      -0 |  ALL  |  ALL  |     1 | Netflix, VOD          |
# | Ometeotl                 | night     |     +12 |      -0 |  ALL  |  ALL  |     1 | Netflix, VOD          |
# | Ometeotl                 | midnight  |     +12 |      -0 |  ALL  |  ALL  |     1 | Netflix, VOD          |
# | Superman                 | day       |     +12 |      -0 |  ALL  |  ALL  |     4 | YouTube, Discipline   |
# | Superman                 | night     |     +12 |      -0 |  ALL  |  ALL  |     4 | YouTube, Discipline   |
# | Superman                 | midnight  |     +12 |      -0 |  ALL  |  ALL  |     4 | YouTube, Discipline   |
# | Upstream                 | day       |     +12 |      -0 |  ALL  |  ALL  |     3 | Alternative, Maeve    |
# | Upstream                 | night     |     +12 |      -0 |  ALL  |  ALL  |     3 | Alternative, Maeve    |
# | Upstream                 | midnight  |     +12 |      -0 |  ALL  |  ALL  |     3 | Alternative, Maeve    |

main() {
  prepare "$@"
  validate "$@"

  case "$SERVICE" in
    "discipline")
      case "$TIMEFRAME" in
        "day")
          todo_personal=$(todo-accomplished --repo "noraworld/reserved-diary" --past 0 --retry 1 --checked 4)
          task_personal=2

          todo_work=1
          task_work=2
          ;;
        "night")
          todo_personal=$(todo-accomplished --repo "noraworld/reserved-diary" --past 0 --retry 1 --checked 6)
          task_personal=2

          todo_work=1
          task_work=2
          ;;
        "midnight")
          todo_personal=$(todo-accomplished --repo "noraworld/reserved-diary" --past 0 --retry 1 --checked 10)
          task_personal=2

          todo_work=1
          task_work=2
          ;;
      esac
      ;;
    "gaming")
      case "$TIMEFRAME" in
        "day")
          todo_personal=$(todo-accomplished --repo "noraworld/reserved-diary" --past 1 --retry 1 --checked 12 --unchecked 0)
          task_personal=$(task-stacked --main-repo "noraworld/reserved-diary" --past 1 --todo-repo "noraworld/to-do")

          todo_work=$(todo-accomplished     --repo "noraworld/daily-report"   --past 1 --retry 1 --unchecked 0)
          task_work=$(task-stacked     --main-repo "noraworld/daily-report"   --past 1 --todo-repo "noraworld/work-scraps")
          ;;
        "night")
          todo_personal=$(todo-accomplished --repo "noraworld/reserved-diary" --past 1 --retry 1 --checked 12 --unchecked 0)
          task_personal=$(task-stacked --main-repo "noraworld/reserved-diary" --past 1 --todo-repo "noraworld/to-do")

          todo_work=$(todo-accomplished     --repo "noraworld/daily-report"   --past 1 --retry 1 --unchecked 0)
          task_work=$(task-stacked     --main-repo "noraworld/daily-report"   --past 1 --todo-repo "noraworld/work-scraps")
          ;;
        "midnight")
          todo_personal=$(todo-accomplished --repo "noraworld/reserved-diary" --past 1 --retry 1 --checked 12 --unchecked 0)
          task_personal=$(task-stacked --main-repo "noraworld/reserved-diary" --past 1 --todo-repo "noraworld/to-do")

          todo_work=$(todo-accomplished     --repo "noraworld/daily-report"   --past 1 --retry 1 --unchecked 0)
          task_work=$(task-stacked     --main-repo "noraworld/daily-report"   --past 1 --todo-repo "noraworld/work-scraps")
          ;;
      esac
      ;;
    "netflix")
      case "$TIMEFRAME" in
        "day")
          todo_personal=$(todo-accomplished --repo "noraworld/reserved-diary" --past 2 --retry 1 --checked 12 --unchecked 0)
          task_personal=$(task-stacked --main-repo "noraworld/reserved-diary" --past 2 --todo-repo "noraworld/to-do")

          todo_work=$(todo-accomplished     --repo "noraworld/daily-report"   --past 2 --retry 1 --unchecked 0)
          task_work=$(task-stacked     --main-repo "noraworld/daily-report"   --past 2 --todo-repo "noraworld/work-scraps")
          ;;
        "night")
          todo_personal=$(todo-accomplished --repo "noraworld/reserved-diary" --past 2 --retry 1 --checked 12 --unchecked 0)
          task_personal=$(task-stacked --main-repo "noraworld/reserved-diary" --past 2 --todo-repo "noraworld/to-do")

          todo_work=$(todo-accomplished     --repo "noraworld/daily-report"   --past 2 --retry 1 --unchecked 0)
          task_work=$(task-stacked     --main-repo "noraworld/daily-report"   --past 2 --todo-repo "noraworld/work-scraps")
          ;;
        "midnight")
          todo_personal=$(todo-accomplished --repo "noraworld/reserved-diary" --past 2 --retry 1 --checked 12 --unchecked 0)
          task_personal=$(task-stacked --main-repo "noraworld/reserved-diary" --past 2 --todo-repo "noraworld/to-do")

          todo_work=$(todo-accomplished     --repo "noraworld/daily-report"   --past 2 --retry 1 --unchecked 0)
          task_work=$(task-stacked     --main-repo "noraworld/daily-report"   --past 2 --todo-repo "noraworld/work-scraps")
          ;;
      esac
      ;;
    "social-media")
      case "$TIMEFRAME" in
        "day")
          todo_personal=$(todo-accomplished --repo "noraworld/reserved-diary" --past 3 --retry 1 --checked 12 --unchecked 0)
          task_personal=$(task-stacked --main-repo "noraworld/reserved-diary" --past 3 --todo-repo "noraworld/to-do")

          todo_work=$(todo-accomplished     --repo "noraworld/daily-report"   --past 3 --retry 1 --unchecked 0)
          task_work=$(task-stacked     --main-repo "noraworld/daily-report"   --past 3 --todo-repo "noraworld/work-scraps")
          ;;
        "night")
          todo_personal=$(todo-accomplished --repo "noraworld/reserved-diary" --past 3 --retry 1 --checked 12 --unchecked 0)
          task_personal=$(task-stacked --main-repo "noraworld/reserved-diary" --past 3 --todo-repo "noraworld/to-do")

          todo_work=$(todo-accomplished     --repo "noraworld/daily-report"   --past 3 --retry 1 --unchecked 0)
          task_work=$(task-stacked     --main-repo "noraworld/daily-report"   --past 3 --todo-repo "noraworld/work-scraps")
          ;;
        "midnight")
          todo_personal=$(todo-accomplished --repo "noraworld/reserved-diary" --past 3 --retry 1 --checked 12 --unchecked 0)
          task_personal=$(task-stacked --main-repo "noraworld/reserved-diary" --past 3 --todo-repo "noraworld/to-do")

          todo_work=$(todo-accomplished     --repo "noraworld/daily-report"   --past 3 --retry 1 --unchecked 0)
          task_work=$(task-stacked     --main-repo "noraworld/daily-report"   --past 3 --todo-repo "noraworld/work-scraps")
          ;;
      esac
      ;;
    "vod")
      case "$TIMEFRAME" in
        "day")
          todo_personal=$(todo-accomplished --repo "noraworld/reserved-diary" --past 2 --retry 1 --checked 12 --unchecked 0)
          task_personal=$(task-stacked --main-repo "noraworld/reserved-diary" --past 2 --todo-repo "noraworld/to-do")

          todo_work=$(todo-accomplished     --repo "noraworld/daily-report"   --past 2 --retry 1 --unchecked 0)
          task_work=$(task-stacked     --main-repo "noraworld/daily-report"   --past 2 --todo-repo "noraworld/work-scraps")
          ;;
        "night")
          todo_personal=$(todo-accomplished --repo "noraworld/reserved-diary" --past 2 --retry 1 --checked 12 --unchecked 0)
          task_personal=$(task-stacked --main-repo "noraworld/reserved-diary" --past 2 --todo-repo "noraworld/to-do")

          todo_work=$(todo-accomplished     --repo "noraworld/daily-report"   --past 2 --retry 1 --unchecked 0)
          task_work=$(task-stacked     --main-repo "noraworld/daily-report"   --past 2 --todo-repo "noraworld/work-scraps")
          ;;
        "midnight")
          todo_personal=$(todo-accomplished --repo "noraworld/reserved-diary" --past 2 --retry 1 --checked 12 --unchecked 0)
          task_personal=$(task-stacked --main-repo "noraworld/reserved-diary" --past 2 --todo-repo "noraworld/to-do")

          todo_work=$(todo-accomplished     --repo "noraworld/daily-report"   --past 2 --retry 1 --unchecked 0)
          task_work=$(task-stacked     --main-repo "noraworld/daily-report"   --past 2 --todo-repo "noraworld/work-scraps")
          ;;
      esac
      ;;
    "youtube")
      case "$TIMEFRAME" in
        "day")
          todo_personal=$(todo-accomplished --repo "noraworld/reserved-diary" --past 3 --retry 1 --checked 12 --unchecked 0)
          task_personal=$(task-stacked --main-repo "noraworld/reserved-diary" --past 3 --todo-repo "noraworld/to-do")

          todo_work=$(todo-accomplished     --repo "noraworld/daily-report"   --past 3 --retry 1 --unchecked 0)
          task_work=$(task-stacked     --main-repo "noraworld/daily-report"   --past 3 --todo-repo "noraworld/work-scraps")
          ;;
        "night")
          todo_personal=$(todo-accomplished --repo "noraworld/reserved-diary" --past 3 --retry 1 --checked 12 --unchecked 0)
          task_personal=$(task-stacked --main-repo "noraworld/reserved-diary" --past 3 --todo-repo "noraworld/to-do")

          todo_work=$(todo-accomplished     --repo "noraworld/daily-report"   --past 3 --retry 1 --unchecked 0)
          task_work=$(task-stacked     --main-repo "noraworld/daily-report"   --past 3 --todo-repo "noraworld/work-scraps")
          ;;
        "midnight")
          todo_personal=$(todo-accomplished --repo "noraworld/reserved-diary" --past 3 --retry 1 --checked 12 --unchecked 0)
          task_personal=$(task-stacked --main-repo "noraworld/reserved-diary" --past 3 --todo-repo "noraworld/to-do")

          todo_work=$(todo-accomplished     --repo "noraworld/daily-report"   --past 3 --retry 1 --unchecked 0)
          task_work=$(task-stacked     --main-repo "noraworld/daily-report"   --past 3 --todo-repo "noraworld/work-scraps")
          ;;
      esac
      ;;
    "youtube-alternative")
      case "$TIMEFRAME" in
        "day")
          todo_personal=$(todo-accomplished --repo "noraworld/reserved-diary" --past 3 --retry 1 --checked 12 --unchecked 0)
          task_personal=$(task-stacked --main-repo "noraworld/reserved-diary" --past 3 --todo-repo "noraworld/to-do")

          todo_work=$(todo-accomplished     --repo "noraworld/daily-report"   --past 3 --retry 1 --unchecked 0)
          task_work=$(task-stacked     --main-repo "noraworld/daily-report"   --past 3 --todo-repo "noraworld/work-scraps")
          ;;
        "night")
          todo_personal=$(todo-accomplished --repo "noraworld/reserved-diary" --past 3 --retry 1 --checked 12 --unchecked 0)
          task_personal=$(task-stacked --main-repo "noraworld/reserved-diary" --past 3 --todo-repo "noraworld/to-do")

          todo_work=$(todo-accomplished     --repo "noraworld/daily-report"   --past 3 --retry 1 --unchecked 0)
          task_work=$(task-stacked     --main-repo "noraworld/daily-report"   --past 3 --todo-repo "noraworld/work-scraps")
          ;;
        "midnight")
          todo_personal=$(todo-accomplished --repo "noraworld/reserved-diary" --past 3 --retry 1 --checked 12 --unchecked 0)
          task_personal=$(task-stacked --main-repo "noraworld/reserved-diary" --past 3 --todo-repo "noraworld/to-do")

          todo_work=$(todo-accomplished     --repo "noraworld/daily-report"   --past 3 --retry 1 --unchecked 0)
          task_work=$(task-stacked     --main-repo "noraworld/daily-report"   --past 3 --todo-repo "noraworld/work-scraps")
          ;;
      esac
      ;;
    "youtube-music-persistent")
      case "$TIMEFRAME" in
        "day")
          todo_personal=$(todo-accomplished --repo "noraworld/reserved-diary" --past 0 --retry 1 --checked 4)
